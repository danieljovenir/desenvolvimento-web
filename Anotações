cd docker
docker compose up -d -> subir o container com o postgres
docker compose stop -> parar o container do postgres

-- public.usuarios definition

-- Drop table

-- DROP TABLE public.usuarios;

CREATE TABLE public.usuarios (
	id serial4 NOT NULL,
	email varchar(255) NOT NULL,
	senha varchar(255) NOT NULL,
	CONSTRAINT usuarios_pkey PRIMARY KEY (id)
);

docker network create lar_das_meninas